{
    "name": "yiithings/yii2-setting",
    "description": "Site setting extension",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","setting"],
    "license": "MIT",
    "authors": [
        {
            "name": "Panlatent",
            "email": "panlatent@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*"
    },
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "yiithings\\setting\\": "src/"
        }
    },
    "require-dev": {
        "yiister/yii2-gentelella": "^1.3"
    }
}
